<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yuanzhiwu.memorydemo">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:name=".MemoryLeakDemoApp"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecycleViewActivity"/>
        <activity android:name=".HierarchyViewActivity"/>
        <activity android:name=".AsyncTaskLeakActivity"/>
        <activity android:name=".AsyncTaskLeakFixActivity"/>
        <activity android:name=".HandlerLeakActivity"/>
        <activity android:name=".HandlerLeakFixActivity"/>
        <activity android:name=".HandlerThreadLeakActivity"/>
        <activity android:name=".LeakActivity"/>
        <activity android:name=".SingletonLeakActivity"/>
        <activity android:name=".SingletonLeakFixActivity"/>
        <activity android:name=".StaticInnerClassLeakActivity"/>
        <activity android:name=".StaticInnerClassLeakFixActivity"/>
        <activity android:name=".ThreadLeakActivity"/>
        <activity android:name=".ThreadLeakFixActivity"/>
        <activity android:name=".BlockCanaryTest"/>
        <activity android:name=".StrictModeTest"/>
        <activity android:name=".LeakCanaryTest"/>
    </application>

</manifest>